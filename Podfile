# minimum version supported
platform :ios, '11.0'

# tells CocoaPods that you want to use frameworks instead of Static Libraries
use_frameworks!

# ignore all warnings from all pods
inhibit_all_warnings!

target 'Reciplease' do
  pod 'Alamofire', '~> 5.2'
  pod 'SVProgressHUD'
  pod 'SwiftLint'
end
